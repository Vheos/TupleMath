<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <OutputType>Exe</OutputType>
    <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
    <CompilerGeneratedFilesOutputPath>Code\AutoGenerated</CompilerGeneratedFilesOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\TupleMathGenerator\TupleMathGenerator.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    <Compile Remove="$(CompilerGeneratedFilesOutputPath)/**/*.cs" />
    <Compile Remove="Code\AutoGenerated\**" />
    <AdditionalFiles Include="$(CompilerGeneratedFilesOutputPath)/**/*.cs" />
  </ItemGroup>

  <ItemGroup>
    <AdditionalFiles Include="Code\AutoGenerated\**" />
    <EmbeddedResource Include="Code\AutoGenerated\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Code\AutoGenerated\TupleMathGenerator\" />
  </ItemGroup>

</Project>
